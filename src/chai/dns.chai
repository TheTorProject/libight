def dns_query(qclass, qtype, query, settings) {
  return [
    "dns_query",
    qclass,
    qtype,
    query,
    settings
  ]
}

var settings = [
  "dns/nameserver": "8.8.8.8:53",
  "dns/timeout": 1.0,
  "dns/attempts": 1
]

yield_from([
  dns_query("IN", "A", "www.google.com", settings),
  dns_query("IN", "A", "www.torproject.org", settings),
  dns_query("IN", "A", "nexa.polito.it", settings)
])
